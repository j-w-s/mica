// basic arithmetic
let x = 10
let y = 20
let z = x + y
print(z)

// variables
let a = 5
let b = a * 2
print(b)

// booleans
let flag = true
print(flag)

// conditionals
if x > 5 {
  print("x is greater than 5")
}

if y < 10 {
  print("y is less than 10")
} else {
  print("y is not less than 10")
}

// loops
let mut i = 0
while i < 5 {
  print(i)
  i = i + 1
}

// arrays
let nums = [1, 2, 3, 4, 5]
print(nums)
print(len(nums))
